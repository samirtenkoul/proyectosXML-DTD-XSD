<?xml version = "1.0" encoding = "UTF-8"?> 
<xsd:schema xmlns:xsd = "http://www.w3.org/2001/XMLSchema">
	<xsd:element name = "alumno">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="nombre" type="string"/>
				<xsd:element name="apellidos" type="string"/>
				<xsd:element name="direccion" type="string"/>
				<xsd:element name="CP" type="integer"/>
				<xsd:element name="edad" type="integer"/>
				<xsd:element name="ciclo_formativo" type="string"/>
				<xsd:element name="clave_ciclo" type="string"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>